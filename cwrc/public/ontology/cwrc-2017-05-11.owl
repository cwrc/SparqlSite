<cwrc:Genre rdf:about="#genreSatire">
   <rdf:value>SATIRE</rdf:value>
   <rdfs:label xml:lang="en">satire</rdfs:label>
   <rdfs:label xml:lang="fr">satire</rdfs:label>
   <rdfs:isDefinedBy rdf:resource="http://www.cwrc.ca/ontologies/cwrc#" />
   <void:inDataset rdf:resource="#datasetdefinition" />
