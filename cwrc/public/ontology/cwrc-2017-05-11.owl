<cwrc:Genre rdf:about="#genreParable">
   <rdf:value>PARABLE</rdf:value>
   <rdfs:label xml:lang="en">parable</rdfs:label>
   <rdfs:label xml:lang="fr" />
   <rdfs:isDefinedBy rdf:resource="http://www.cwrc.ca/ontologies/cwrc#" />
